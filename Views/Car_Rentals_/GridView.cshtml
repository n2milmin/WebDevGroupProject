@model List<Car_Rental>


<div class="col-sm-12">
	<table class="table">
		<thead>
			<tr>
				<td>Company Name</td>
				<td>Location</td>
				<td>Model</td>
				<td>Price</td>
				<td>Availablility</td>
			</tr>
		</thead>
		<tbody>
			@if(Model.Count() != 0)
			{
				@foreach (var x in Model)
				{
					<tr>
						<td>@x.CompanyName</td>
						<td>@x.Location</td>
						<td>@x.Model</td>
						<td>@x.Pricing</td>
						<td>@x.Availability</td>
					</tr>
				}
			}
			else
			{
				<tr>
					<td colspan="5" class="text-center">No Matches</td>
				</tr>
			}
		</tbody>
	</table>
</div>
